{
  "version": "0.2.0",
  "configurations": [
      {
          "name": "Debug C Project",
          "type": "cppdbg",
          "request": "launch",
          "program": "${workspaceFolder}/push_swap",
          "args": ["12", "8", "5", "20", "1", "15", "19", "2", "11", "9"],
          "stopAtEntry": false,
          "cwd": "${workspaceFolder}",
          "environment": [],
          "externalConsole": false,
          "MIMode": "gdb",
          "setupCommands": [
              {
                  "description": "Enable pretty-printing for gdb",
                  "text": "-enable-pretty-printing",
                  "ignoreFailures": true
              }
          ],
      }
  ]
}